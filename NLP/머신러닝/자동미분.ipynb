      "source": [
        "import tensorflow as tf"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "w = tf.Variable(2.) # 변수 w 정의\n",
        "\n",
        "def f(w):\n",
        "  # 함수 f 정의\n",
        "  y = w**2\n",
        "  z = 2*y + 5\n",
        "  return z"
      ],
      "metadata": {
        "id": "L7_OrUDVUH-T"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "with tf.GradientTape() as tape:\n",
        "  # tf.GeadientTape 블록을 만들어 관련된 연산을 기록\n",
        "  z = f(w)\n",
        "\n",
        "gradient = tape.gradient(z, [w]) # 그래디언트 확인\n",
        "print(gradient)"
      ],
